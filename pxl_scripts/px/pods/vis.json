{
  "variables": [
    {
      "name": "start_time",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    },
    {
      "name": "namespace",
      "type": "PX_NAMESPACE",
      "description": "The namespace to filter on",
      "defaultValue": "default"
    }
  ],
  "globalFuncs": [
    {
      "outputName": "inbound_let_timeseries",
      "func": {
        "name": "inbound_let_timeseries",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "namespace",
            "variable": "namespace"
          }
        ]
      }
    },
    {
      "outputName": "resource_timeseries",
      "func": {
        "name": "resource_timeseries",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "namespace",
            "variable": "namespace"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "Pods List",
      "func": {
        "name": "pods",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "namespace",
            "variable": "namespace"
          }
        ]
      },
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.Table"
      },
      "position": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 3
      }
    },
    {
      "name": "HTTP Requests",
      "globalFuncOutputName": "inbound_let_timeseries",
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "requests_per_s",
            "series": "pod",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "requests per second"
        },
        "xAxis": null
      },
      "position": {
        "x": 8,
        "y": 0,
        "w": 4,
        "h": 3
      }
    },
